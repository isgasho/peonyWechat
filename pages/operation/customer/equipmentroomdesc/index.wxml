<!--pages/operation/customer/equipmentroomdesc/index.wxml-->
<view class="page">
  <view class="page__hd">
    <view class="page__title">{{item.RoomName}}</view>
    <view class="page__desc">电房信息</view>
  </view>
  <view class="page__bd">
    <view class="weui-form-preview">
      <view class="weui-form-preview__bd">
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">电房编号</view>
          <view class="weui-form-preview__value">{{item.RoomNo}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">电房名称</view>
          <view class="weui-form-preview__value">{{item.RoomName}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">客户编号</view>
          <view class="weui-form-preview__value">{{item.CustomerNo}}</view>
        </view>
       
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">状态</view>
          <switch class="weui-form-preview__value" disabled="true" checked="{{item.Tag}}"/> 
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">创建人</view>
          <view class="weui-form-preview__value">{{item.Createuser}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">创建时间</view>
          <view class="weui-form-preview__value">{{item.Createdate}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">修改人</view>
          <view class="weui-form-preview__value">{{item.Changeuser}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">修改时间</view>
          <view class="weui-form-preview__value">{{item.Changedate}}</view>
        </view>
      </view>
    </view>
  </view>
</view>
